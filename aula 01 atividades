void main() {
  print("Programa Dart iniciado");

  String saudacao = "Olá";
  String local = "mundo";

  int anoAtual = 2024;

  bool estaChovendo = false;

  String nome = "Henrique";
  String sobrenome = "Silva";
  String nomeCompleto = "$nome $sobrenome";

  print("Meu nome é $nomeCompleto");

  int velocidade = 85; // Exemplo de velocidade em km/h
  if (velocidade > 80) {
    print("Você foi multado por excesso de velocidade.");
  } else {
    print("Você está dentro do limite de velocidade.");
  }

  int anoNascimento = 2005;
  int idade = anoAtual - anoNascimento;
  
  if (idade >= 16) {
    print("Você tem $idade anos e pode votar.");
  } else {
    print("Você tem $idade anos e não pode votar.");
  }
}
